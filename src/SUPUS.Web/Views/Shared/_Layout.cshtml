@using SUPUS.Abstraction

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="~/Content/jQuery/jquery-3.3.1.min.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap.js"></script>
    <script src="~/Content/Bootstrap/js/bootstrap.bundle.js"></script>
    <link href="~/Content/Bootstrap/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-dark" style="background-color: #ed1c24;">
        <a class="navbar-brand" asp-action="Index" asp-controller="Home">SUPUS</a>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" asp-action="Index" asp-controller="Home">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-action="Index" asp-controller="Employee">Employees</a>
                </li>
            </ul>
        </div>
    </nav>
<div class="container">
    @RenderBody()
    <a href="~/Views/Shared/_Layout.cshtml">~/Views/Shared/_Layout.cshtml</a>
</div>
<hr/>
<footer>
    <p>&copy; 2019 - SUPUS</p>
</footer>
</body>
</html>

